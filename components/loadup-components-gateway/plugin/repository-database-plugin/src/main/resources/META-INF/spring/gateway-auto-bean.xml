<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName">

    <bean id="gatewayinstDAO"
          class="com.github.loadup.components.gateway.plugin.repository.database.dal.ibatis.IbatisInstDAO"
          parent="gatewaySqlMapClientDAO"/>

    <bean id="gatewayinstInterfaceMapDAO"
          class="com.github.loadup.components.gateway.plugin.repository.database.dal.ibatis.IbatisInstInterfaceMapDAO"
          parent="gatewaySqlMapClientDAO"/>

    <bean id="gatewayinterfaceDAO"
          class="com.github.loadup.components.gateway.plugin.repository.database.dal.ibatis.IbatisInterfaceDAO"
          parent="gatewaySqlMapClientDAO"/>

    <bean id="gatewaysecurityDAO"
          class="com.github.loadup.components.gateway.plugin.repository.database.dal.ibatis.IbatisSecurityDAO"
          parent="gatewaySqlMapClientDAO"/>

</beans>
