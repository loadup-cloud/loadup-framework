<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMap namespace="acts">

    <resultMap id="RM-ORG-DB" class="com.alipay.test.acts.dal.dataobject.OrgDbDO">
        <result property="sourceId" column="source_id" javaType="java.lang.String" jdbcType="VARCHER2" />
        <result property="system" column="system" javaType="java.lang.String" jdbcType="VARCHER2" />
        <result property="sourceData" column="source_data" javaType="java.lang.String" jdbcType="VARCHER2" />
        <result property="sourceRule" column="source_rule" javaType="java.lang.String" jdbcType="VARCHER2"/>
        <result property="gmtCreate" column="gmt_create" javaType="java.util.Date" jdbcType="TIMESTAMP(6)"/>
        <result property="gmtModify" column="gmt_modify" javaType="java.util.Date" jdbcType="TIMESTAMP(6)"/>
        <result property="memo" column="memo" javaType="java.lang.String" jdbcType="VARCHER2" />
    </resultMap>
		
	<select id="RM-ORG-DB-BY-SYSTEM-AND-SOURCE-DATA" resultMap="RM-ORG-DB" >
			<![CDATA[
				select source_id,
						system,
						source_data,
						source_rule,
						memo,
						gmt_create,
						gmt_modify
			  from org_db
			 where system=#system# and source_data=#sourceData#
				]]>
	</select>
	
	<select id="RM-ORG-DB-BY-SOURCE-ID" resultMap="RM-ORG-DB" >
			<![CDATA[
				select source_id,
						system,
						source_data,
						source_rule,
						memo,
						gmt_create,
						gmt_modify
			  from org_db
			 where source_id=#sourceId#
				]]>
	</select>
	
	<select id="RM-ORG-DB-BY-SYSTEM" resultMap="RM-ORG-DB"  >
			<![CDATA[
				select source_id,
						system,
						source_data,
						source_rule,
						memo,
						gmt_create,
						gmt_modify
			  from org_db
			 where system=#system#
				]]>
	</select>
	
</sqlMap>
