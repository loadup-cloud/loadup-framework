<?xml version="1.0" encoding="UTF-8"?>
<!--
  OWASP Dependency-Check 抑制配置
  用于抑制已知误报或已接受风险的 CVE
  文档: https://jeremylong.github.io/DependencyCheck/general/suppression.html
-->
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!--
      示例：抑制特定 CVE
      在确认 CVE 不影响本项目的使用场景后，在此添加抑制规则，并注明原因和负责人
    -->

    <!--
    <suppress until="2027-01-01">
        <notes>
          CVE-2024-XXXXX: 该漏洞影响 xxx 模块的 yyy 功能，本项目未使用该功能。
          确认人: @developer, 日期: 2026-02-28
        </notes>
        <cve>CVE-2024-XXXXX</cve>
    </suppress>
    -->

    <!-- 测试依赖（testcontainers 仅用于测试环境，不在生产部署） -->
    <suppress>
        <notes>测试容器相关依赖仅用于 CI/CD 测试环境，不部署到生产</notes>
        <packageUrl regex="true">^pkg:maven/org\.testcontainers/.*$</packageUrl>
        <cvssBelow>7</cvssBelow>
    </suppress>

</suppressions>

