# MyBatis-Flex APT Processor Configuration
# This file controls the generation of TableDef classes through annotation processing

# Enable automatic generation of Tables class containing all TableDef
mybatisflex.processor.tables-generate-enable=true

# Enable generation of individual TableDef classes for each entity
mybatisflex.processor.entity-generate-enable=false

# Generate AllInTables (Tables class contains all table definitions)
mybatisflex.processor.all-in-tables-enable=true

# Table class name (default: Tables)
mybatisflex.processor.tables-class-name=Tables

# Mapper generator enable (if needed)
mybatisflex.processor.mapper-generate-enable=false


